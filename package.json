{
  "name": "migration-assessment",
  "private": false,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "fec build",
    "clean": "rimraf node_modules dist .cache",
    "copy-static": "cp -r public/* dist/",
    "deploy": "npm-run-all build lint test",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src --ext .js,.jsx,.ts,.tsx --cache",
    "lint:js:fix": "eslint src --ext .js,.jsx,.ts,.tsx --cache --fix",
    "patch:hosts": "fec patch-etc-hosts",
    "start": "HOT=true fec dev --clouddotEnv=stage --uiEnv=stable",
    "start:dev-proxy": "fec dev-proxy --clouddotEnv=stage --uiEnv=stable",
    "start:federated": "fec static",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "postbuild": "npm run copy-static",
    "postinstall": "ts-patch install && rimraf .cache",
    "verify": "npm-run-all build lint test",
    "prettier": "prettier --write src",
    "prettier:check": "prettier --check src",
    "start:standalone": "webpack serve --config webpack.standalone.js",
    "build:standalone": "webpack --config webpack.standalone.js --mode production",
    "serve:standalone": "serve dist-standalone -p 3001"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@migration-planner-ui/api-client": "^0.0.32",
    "@migration-planner-ui/ioc": "^0.0.31",
    "@patternfly/react-charts": "7.4.9",
    "@patternfly/react-core": "^6.2.2",
    "@patternfly/react-icons": "6.4.0",
    "@patternfly/react-table": "^6.2.2",
    "@patternfly/react-tokens": "^6.2.2",
    "@redhat-cloud-services/frontend-components": "^6.0.7",
    "@redhat-cloud-services/frontend-components-notifications": "^6.0.7",
    "@redhat-cloud-services/frontend-components-utilities": "^6.0.7",
    "github-markdown-css": "^5.8.1",
    "html2canvas-pro": "^1.6.0",
    "humanize-plus": "^1.8.2",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-content-loader": "^6.2.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.26.2",
    "react-syntax-highlighter": "^16.1.0",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.0",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.4",
    "@redhat-cloud-services/frontend-components-config": "^6.7.4",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.26",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@types/humanize-plus": "^1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jsdom": "^27.3.0",
    "npm-run-all2": "^5.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.10",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.6.2",
    "vitest": "^4.0.16",
    "webpack": "^5.x.x",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "overrides": {
    "@redhat-cloud-services/frontend-components-config": {
      "webpack-dev-server": ">=5.2.1"
    },
    "@redhat-cloud-services/tsc-transform-imports": {
      "glob": ">=10.4.6"
    }
  },
  "insights": {
    "appname": "assisted-migration-app"
  }
}
