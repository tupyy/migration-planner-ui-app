{
  "name": "migration-assessment",
  "version": "0.0.1",
  "private": false,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "fec build",
    "clean": "rimraf node_modules dist .cache",
    "copy-static": "cp -r public/* dist/",
    "deploy": "npm-run-all build lint test",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src",
    "lint:js:fix": "eslint src --fix",
    "patch:hosts": "fec patch-etc-hosts",
    "start": "HOT=true fec dev --clouddotEnv=stage --uiEnv=stable",
    "start:federated": "fec static",
    "test": "TZ=UTC jest --verbose --no-cache --passWithNoTests",
    "postbuild": "npm run copy-static",
    "postinstall": "ts-patch install && rimraf .cache",
    "verify": "npm-run-all build lint test",
    "prettier": "prettier --write src",
    "start:standalone": "webpack serve --config webpack.standalone.js",
    "build:standalone": "webpack --config webpack.standalone.js --mode production",
    "serve:standalone": "serve dist-standalone -p 3001"
  },
  "dependencies": {
    "@emotion/css": "^11.13.0",
    "@migration-planner-ui/agent-client": "^0.0.15",
    "@migration-planner-ui/api-client": "^0.0.15",
    "@migration-planner-ui/ioc": "^0.0.15",
    "@patternfly/react-charts": "7.4.1",
    "@patternfly/react-core": "^5.4.1",
    "@patternfly/react-table": "^5.4.1",
    "@redhat-cloud-services/frontend-components": "^4.2.16",
    "@redhat-cloud-services/frontend-components-notifications": "^4.1.0",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.17",
    "dotenv": "^16.4.5",
    "github-markdown-css": "^5.8.1",
    "html2canvas": "^1.4.1",
    "humanize-plus": "^1.8.2",
    "jotai": "^2.10.0",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-content-loader": "^6.2.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.26.2",
    "react-syntax-highlighter": "^15.6.1",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.4",
    "@redhat-cloud-services/frontend-components-config": "^6.3.0",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.16",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@types/humanize-plus": "^1",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "css-loader": "^6.x.x",
    "eslint": "^8.57.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "html-webpack-plugin": "^5.x.x",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.1",
    "rimraf": "^5.0.10",
    "sass-loader": "^13.3.3",
    "serve": "^14.x.x",
    "style-loader": "^3.x.x",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-patch": "^3.2.1",
    "typescript": "^5.6.2",
    "webpack": "^5.x.x",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "^4.x.x",
    "webpack-dev-server": "^4.x.x"
  },
  "insights": {
    "appname": "assisted-migration-app"
  }
}
